{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server",
  "scripts": {
    "build": "/bin/sh tsc_eval.sh",
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*.{test,spec}.ts -t 10000 --exit",
    "test:coverage": "nyc npm run test",
    "test:coverage:json": "nyc --reporter=json-summary npm run test",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "prestart": "npm run db:reset",
    "start": "node .",
    "predev": "npm run build && npm run db:reset",
    "dev": "nodemon --watch \"./src/**\" ./src/server.ts",
    "postinstall": "if [ ! -z 'cat ./packages.npm' ]; then npm i --ignore-scripts $(cat ./packages.npm); fi",
    "lint": "eslint ./src --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/sequelize": "^4.28.14",
    "eslint": "6.8.0",
    "eslint-config-trybe-backend": "1.0.1",
    "mocha": "^10.2.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/restify-errors": "^4.3.4",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "restify-errors": "^8.0.2",
    "sequelize": "^6.28.0",
    "sinon": "^15.0.1"
  }
}
